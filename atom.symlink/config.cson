"*":
  editor:
    fontFamily: "Menlo"
    fontSize: 10
    showIndentGuide: true
    tabLength: 4
    invisibles: {}
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    excludeVcsIgnoredPaths: false
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "omnisharp-atom"
    ]
    themes: [
      "unity-ui"
      "atom-dark-syntax"
    ]
    followSymlinks: false
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  welcome:
    showOnStartup: false
  "release-notes":
    viewedVersion: "0.89.0"
  "remote-edit":
    showHiddenFiles: true
  linter:
    lintOnSave: false
    lintOnChange: false
    lintOnEditorFocus: false
    showHighlighting: false
    showGutters: false
    showErrorInline: false
  "linter-tidy":
    tidyExecutablePath: "usr/bin/tidy"
  "atom-beautify":
    _analyticsUserId: "6fe7c5c7-8c61-4c6c-ac3b-a4bd3fe33475"
  "terminal-status": {}
  "autocomplete-plus": {}
  "omnisharp-atom":
    enhancedHighlighting: true
    useLeftLabelColumnForSuggestions: true
  "go-plus":
    goPath: "/Users/holman/Code/go"

